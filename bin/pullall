#!/bin/bash
# get all the files in tangomapper recorded dataset.
for file in `adb shell ls /data/data/com.motorola.atap.tangomapper/files`; do
  echo "processing dataset $file"
  filepath=`echo "/smb/scratch/03-PEOPLE/fcampose/yellowstone_datasets/$file" | tr '\r' ' '`
  echo "writing to $filepath"
  mkdir -p $filepath
  /home/fede/bin/adbfastpull -o$filepath $file
done

